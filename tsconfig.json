{
	"compilerOptions": {
		"outDir": "./dist",
		"module": "nodenext",
		"target": "esnext",
		"types": [],
		"lib": [
			"esnext"
		],
		"types": [
			"node"
		],
		"sourceMap": false,
		"declaration": false,
		"declarationMap": false,
		"noUncheckedIndexedAccess": true,
		"exactOptionalPropertyTypes": true,
		"strict": true,
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"noUncheckedSideEffectImports": true,
		"moduleDetection": "force",
		"skipLibCheck": true
	}
}
